{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node' ./src/index.ts",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "test:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "test:setup": "docker compose up -d && sleep 1 && npm run test:migrate",
    "test:clear": "docker compose down",
    "test:integration": "npm run test:clear && dotenv -e .env.test -- npm run test:setup && jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^8.4.2",
    "@fastify/jwt": "^7.2.4",
    "@fastify/swagger": "^8.12.1",
    "@fastify/swagger-ui": "^2.0.1",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@hapi/boom": "^10.0.1",
    "@prisma/client": "^5.7.0",
    "@sinclair/typebox": "^0.31.28",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^2.0.16",
    "@whiskeysockets/baileys": "^6.5.0",
    "bcrypt": "^5.1.1",
    "fastify": "^4.25.0",
    "fastify-plugin": "^4.5.1",
    "fastify-sse-v2": "^3.1.2",
    "jest": "^29.7.0",
    "link-preview-js": "^3.0.5",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "ts-jest": "^29.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.10.4",
    "@types/qrcode": "^1.5.5",
    "dotenv-cli": "^7.3.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "prisma-dbml-generator": "^0.10.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
