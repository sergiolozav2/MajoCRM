{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "typescript-eslint/semi": false,
    "typescript-eslint/explicit-function-return-type": 0,
    "typescript-eslint/comma-dangle": false
  }
}
